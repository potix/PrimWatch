{
    "logging": {
        "type": "file",
        "file": "/var/log/primwatch/healthcheck.log",
        "level": "DEBUG"
    },
    "workers": 4,
    "default_timeout": 5,
    "default_retry": 1,
    "targets": {
        "local": {
            "address": "127.0.0.1",
            "watches": {
                "ICMP": {
                    "retry": 5,
                    "timeout": 3
                },
                "URL": {
                    "codes": [200],
                    "url": "http://localhost:8080"
                }
            }
        },
        "vm00": {
